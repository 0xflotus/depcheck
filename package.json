  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "lodash": "^3.3.0",
    "minimatch": "^2.0.1",
    "node-source-walk": "^2.0.0",
    "optimist": "~0.6.0",
    "q": "^1.0.1",
    "walkdir": "0.0.7"
  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "istanbul": "^0.3.13",
    "mocha": "^2.1.0",
    "mocha-lcov-reporter": "0.0.2",
    "should": "^5.0.1"
  }
