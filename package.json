{
  "name": "depcheck-es6",
  "version": "0.5.2",
  "description": "Check dependencies in your node module",
  "main": "index",
  "bin": {
    "depcheck": "bin/depcheck"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/lijunle/depcheck-es6.git"
  },
  "scripts": {
    "test": "mocha test/index.js",
    "test-coverage": "istanbul cover node_modules/mocha/bin/_mocha test/index.js",
    "test-coveralls": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- -R spec"
  },
  "author": [
    "Djordje Lukic",
